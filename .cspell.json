{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "dictionaries": ["unity", "csharp", "softwareTerms", "companies", "fonts"],
  "dictionaryDefinitions": [
    {
      "name": "unity",
      "path": ".cspell/unity-dictionary.txt",
      "addWords": true
    },
    {
      "name": "csharp",
      "path": ".cspell/csharp-dictionary.txt",
      "addWords": true
    }
  ],
  "ignorePaths": ["node_modules/**", ".git/**", "*.min.js", "package-lock.json", ".cspell/**"],
  "ignoreRegExpList": [
    "/```[\\s\\S]*?```/g",
    "\\[.*?\\]\\(.*?\\)",
    "https?://[^\\s)>]+",
    "`[^`]+`"
  ],
  "words": [
    "Animancer",
    "asyncs",
    "behaviour",
    "behaviours",
    "buildable",
    "cancelable",
    "coroutine",
    "coroutines",
    "deserialize",
    "deserialized",
    "despawn",
    "despawned",
    "despawning",
    "deterministically",
    "dxmessaging",
    "editorconfig",
    "gameobject",
    "gameobjects",
    "graphy",
    "hotkey",
    "hotkeys",
    "hotreload",
    "lerp",
    "lerped",
    "lerping",
    "lifecycle",
    "monobehaviour",
    "monobehaviours",
    "odin",
    "performant",
    "playerprefs",
    "poolable",
    "primetween",
    "prefab",
    "prefabs",
    "raycast",
    "raycasts",
    "rebindable",
    "reimport",
    "rigidbody",
    "rigidbodies",
    "runtimes",
    "scriptable",
    "scriptableobject",
    "scriptableobjects",
    "serializable",
    "serializer",
    "sirenix",
    "slerp",
    "struct",
    "structs",
    "subfolder",
    "subfolders",
    "timescale",
    "tweens",
    "tweening",
    "unparent",
    "unparented",
    "unpool",
    "unpooled",
    "wallstop"
  ]
}
